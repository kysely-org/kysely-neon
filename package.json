{
  "name": "kysely-neon",
  "version": "1.3.0",
  "description": "Neon dialect for Kysely",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "check:exports": "attw . --pack",
    "typecheck": "tsc --noEmit",
    "test": "ava"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "neon",
    "neondatabase",
    "postgres",
    "pg",
    "serverless",
    "websockets",
    "ws",
    "kysely",
    "query builder",
    "sql",
    "typescript",
    "database",
    "type-safe",
    "dialect"
  ],
  "author": "Severin Ibarluzea",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@neondatabase/serverless": "^0.4.26",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^18.14.6",
    "@types/ws": "^8.5.4",
    "kysely": "^0.28.0",
    "prettier": "^2.8.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "ws": "^8.13.0"
  },
  "peerDependencies": {
    "@neondatabase/serverless": "^0.4.26",
    "kysely": "^0.28.0",
    "ws": "^8.13.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
