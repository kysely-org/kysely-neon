{
  "name": "kysely-neon",
  "version": "2.0.1",
  "description": "Kysely dialect for Neon",
  "type": "module",
  "repository": "git://github.com/kysely-org/kysely-neon.git",
  "homepage": "https://kysely.dev",
  "author": "Severin Ibarluzea",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "neon",
    "neondatabase",
    "postgres",
    "pg",
    "serverless",
    "kysely",
    "query builder",
    "sql",
    "typescript",
    "database",
    "type-safe",
    "dialect"
  ],
  "scripts": {
    "biome": "biome",
    "build": "tsup",
    "check:exports": "attw . --pack",
    "check:types": "tsc --noEmit",
    "prepublishOnly": "biome check && pnpm build && pnpm check:exports",
    "test": "vitest"
  },
  "peerDependencies": {
    "@neondatabase/serverless": "^1",
    "kysely": "^0.28"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.3.0",
    "@neondatabase/serverless": "^1.0.2",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.3.0",
    "@types/ws": "^8.18.1",
    "kysely": "^0.28.8",
    "std-env": "^3.10.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3",
    "ws": "^8.18.3"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.19.1-oidc-test.3+sha512.e5961a4b56bb37620f97b65da4ca56216634bf39ed358eda925f0f036534d458fa249d93ed0021410523f8bb7ed8512b61fe5532ed3e81777793366c6be026b5"
}
